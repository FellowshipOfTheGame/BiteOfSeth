    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: 8baef7e1267c7bf4283c98bc72fc830f, type: 2}
    myText: "All right! Thanks for your time.\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: 8baef7e1267c7bf4283c98bc72fc830f, type: 2}
