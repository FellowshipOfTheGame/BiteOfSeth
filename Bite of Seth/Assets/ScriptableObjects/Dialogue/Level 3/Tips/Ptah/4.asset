    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: 8baef7e1267c7bf4283c98bc72fc830f, type: 2}
    myText: "Headaches? Nose bleeding? Insomnia? No?\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: 8baef7e1267c7bf4283c98bc72fc830f, type: 2}
    myText: "Are you able to turn it on and off properly? Or weird voices keep talking
