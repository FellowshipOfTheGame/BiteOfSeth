  dialogueID: 0
  dialogueTitle: Maat
  dialogueInfo:
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: 'Nubia, are you still there?

'
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "My assessors have a trial scheduled for less than an hour... to judge
      the Pharaoh's heart\r."
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "This will, of course, not interfere with your trial, since you are not
      dead.\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "I know you enjoy talking with the divine, now from my own experience!\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "But I also know why you are here in the first place.\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "There is no way to prevent this story from ending, but now is the time
      to change its ending.\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "I don't know if you will choose me as your advocate, but I seek justice
      regardless\r."
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "And I know the Nubia who stands before me right now deserves the chance.\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "She just has to make the right choice.\r"
    voicedLine: {fileID: 0}
  - needPuzzleInfo: 0
    character: {fileID: 11400000, guid: f39bade18e66b384993cc343b9bc4428, type: 2}
    myText: "So think\r."
    voicedLine: {fileID: 0}
